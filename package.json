{
  "name": "@devnetic/cli",
  "version": "2.0.0",
  "description": "A tool for rapidly parse command line arguments or options",
  "main": "index.js",
  "unpkg": "index.js",
  "scripts": {
    "build": "npm run clean && tsc",
    "build:watch": "npm run build -- -w",
    "clean": "rm -rf lib",
    "coverage": "nyc ava",
    "lint": "ts-standard",
    "lint:fix": "ts-standard --fix",
    "prepublishOnly": "npm run build && npm run lint && npm run test",
    "release": "standard-version",
    "test": "ava",
    "test:watch": "ava --watch",
    "compile": "tsc"
  },
  "ava": {
    "files": [
      "test/**/*.spec.js"
    ]
  },
  "ts-standard": {
    "ignore": [
      "lib",
      "examples"
    ]
  },
  "keywords": [
    "cli",
    "command line",
    "opts",
    "parseopt",
    "opt",
    "args",
    "console",
    "argsparse",
    "optparse"
  ],
  "files": [
    "src/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/devnetic/cli.git"
  },
  "author": "Álvaro José Agámez Licha <alvaroagamez@outlook.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/devnetic/cli/issues"
  },
  "homepage": "https://github.com/devnetic/cli#readme",
  "devDependencies": {
    "@commitlint/cli": "^9.0.1",
    "@commitlint/config-conventional": "^9.0.1",
    "@types/node": "^14.0.13",
    "ava": "^3.9.0",
    "husky": "^4.2.5",
    "nyc": "^15.1.0",
    "standard-version": "^8.0.0",
    "ts-standard": "^8.0.1",
    "typescript": "^3.9.5"
  },
  "dependencies": {
    "@devnetic/utils": "^1.2.0"
  }
}
